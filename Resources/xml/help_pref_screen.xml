<?xml version="1.0" encoding="utf-8" ?>
<PreferenceScreen
    xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:android="http://schemas.android.com/apk/res/android"
	app:title="Help"
	app:key="help">

	<Preference
        app:key="faq"
        app:title="FAQ"
		app:icon="@drawable/ic_help">
		<intent android:action="android.intent.action.VIEW"
				android:data="http://www.graylabs.com/faq"/>
	</Preference>

	<Preference
        app:key="contact_us"
        app:title="Contact us"
		app:summary="Need help? How we manage your data?"
		app:icon="@drawable/ic_contact_us">
		<intent android:action="android.intent.action.VIEW"
				android:data="http://www.graylabs.com/contactus"/>
	</Preference>

	<Preference
        app:key="terms"
        app:title="Terms and Privacy Policy"
		app:icon="@drawable/ic_file">
		<intent android:action="android.intent.action.VIEW"
				android:data="http://www.graylabs.com/terms"/>
	</Preference>

	<Preference
        app:key="app_info"
        app:title="App info"
		app:icon="@drawable/ic_info">
		<intent
			android:targetPackage="com.graylabs.oyadieyie3d"
			android:targetClass="id.Oyadieyie3D.Activities.AboutActivity"/>
	</Preference>

</PreferenceScreen>